<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
			xmlns:cim="http://iec.ch/TC57/2010/CIM-schema-cim15#"
			xmlns:ftn="http://www.ftnyddro.com/CIM15/2010/extension#">

	<!-- ConnectivityNode -->
	<cim:ConnectivityNode rdf:ID="CN_1">
		<cim:ConnectivityNode.description>ConNode_1</cim:ConnectivityNode.description>
		<cim:IdentifiedObject.mRID>CN_1</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>CN_1</cim:IdentifiedObject.name>
	</cim:ConnectivityNode>
	
	<cim:ConnectivityNode rdf:ID="CN_2">
		<cim:ConnectivityNode.description>ConNode_2</cim:ConnectivityNode.description>
		<cim:IdentifiedObject.mRID>CN_2</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>CN_2</cim:IdentifiedObject.name>
	</cim:ConnectivityNode>
	
	<cim:ConnectivityNode rdf:ID="CN_3">
		<cim:ConnectivityNode.description>ConNode_3</cim:ConnectivityNode.description>
		<cim:IdentifiedObject.mRID>CN_3</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>CN_3</cim:IdentifiedObject.name>
	</cim:ConnectivityNode>
				
	<!-- TERMINAL -->
	<cim:Terminal rdf:ID="T_1">
		<cim:Terminal.ConnectivityNode rdf:resource="#CN_1"/>
		<cim:Terminal.ConductingEquipment rdf:resource="#DCLS_1"/>
		<cim:IdentifiedObject.mRID>T_1</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>T_1</cim:IdentifiedObject.name>
	</cim:Terminal>
	
	<cim:Terminal rdf:ID="T_2">
		<cim:Terminal.ConnectivityNode rdf:resource="#CN_1"/>
		<cim:IdentifiedObject.mRID>T_2</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>T_2</cim:IdentifiedObject.name>
	</cim:Terminal>
	
	<cim:Terminal rdf:ID="T_3">
		<cim:Terminal.ConnectivityNode rdf:resource="#CN_2"/>
		<cim:IdentifiedObject.mRID>T_3</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>T_3</cim:IdentifiedObject.name>
	</cim:Terminal>
	
	<cim:Terminal rdf:ID="T_4">
		<cim:Terminal.ConnectivityNode rdf:resource="#CN_2"/>
		<cim:IdentifiedObject.mRID>T_4</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>T_4</cim:IdentifiedObject.name>
	</cim:Terminal>


	<!-- ACLineSegment -->	
	<cim:ACLineSegment rdf:ID="ACLS_2"> 
		<cim:ACLineSegment.PerLengthImpedance rdf:resource="#PLSI_1"/>
		<cim:ACLineSegment.b0ch>156</cim:ACLineSegment.b0ch>
		<cim:ACLineSegment.bch >65</cim:ACLineSegment.bch>
		<cim:ACLineSegment.g0ch>32</cim:ACLineSegment.g0ch>
		<cim:ACLineSegment.gch>123</cim:ACLineSegment.gch>
		<cim:ACLineSegment.r>11</cim:ACLineSegment.r>
		<cim:ACLineSegment.r0>7</cim:ACLineSegment.r0>
		<cim:ACLineSegment.x>15</cim:ACLineSegment.x>
		<cim:ACLineSegment.x0>10</cim:ACLineSegment.x0>
		<cim:Conductor.length>150</cim:Conductor.length>
		<cim:IdentifiedObject.aliasName>ACLS_2</cim:IdentifiedObject.aliasName>
		<cim:IdentifiedObject.mRID>ACLS_2</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>ACLS_2</cim:IdentifiedObject.name>
	</cim:ACLineSegment>

	

	<!-- DCLineSegment -->
	<cim:DCLineSegment rdf:ID="DCLS_1">
		<cim:Conductor.length>0.7</cim:Conductor.length>
		<cim:ConductingEquipment.Terminals>T_1</cim:ConductingEquipment.Terminals>
		<cim:IdentifiedObject.mRID>DCLS_1</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>DCLS_1</cim:IdentifiedObject.name>
	</cim:DCLineSegment>

	<!-- PerLengthSequenceImpedance -->
	<cim:PerLengthSequenceImpedance rdf:ID="PLSI_1">
		<cim:PerLengthSequenceImpedance.b0ch>12</cim:PerLengthSequenceImpedance.b0ch>
		<cim:PerLengthSequenceImpedance.bch>13.5</cim:PerLengthSequenceImpedance.bch>
		<cim:PerLengthSequenceImpedance.g0ch>136</cim:PerLengthSequenceImpedance.g0ch>
		<cim:PerLengthSequenceImpedance.gch>111.95</cim:PerLengthSequenceImpedance.gch>
		<cim:PerLengthSequenceImpedance.r>12</cim:PerLengthSequenceImpedance.r>
		<cim:PerLengthSequenceImpedance.r0>13.6</cim:PerLengthSequenceImpedance.r0>
		<cim:PerLengthSequenceImpedance.x>5</cim:PerLengthSequenceImpedance.x>
		<cim:PerLengthSequenceImpedance.x0>7.8</cim:PerLengthSequenceImpedance.x0>
		<cim:IdentifiedObject.mRID>PLSI_1</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>PLSI_1</cim:IdentifiedObject.name>
		<!-- <cim:PerLengthImpedance.ACLineSegment rdf:resource="#ACLS_2"/> -->
	</cim:PerLengthSequenceImpedance>

	<!-- SeriesCompensator -->
	<cim:SeriesCompensator rdf:ID="SC_1">
		<cim:SeriesCompensator.r>5</cim:SeriesCompensator.r>
		<cim:SeriesCompensator.r0>15.56</cim:SeriesCompensator.r0>
		<cim:SeriesCompensator.x>7</cim:SeriesCompensator.x>
		<cim:SeriesCompensator.x0>7.96</cim:SeriesCompensator.x0>
		<cim:IdentifiedObject.mRID>SC_1</cim:IdentifiedObject.mRID>
		<cim:IdentifiedObject.name>SC_1</cim:IdentifiedObject.name>
	</cim:SeriesCompensator> 

</rdf:RDF>